{
  "name": "mandelbrot-viewer",
  "version": "1.0.0",
  "description": "a mandelbrot visualizer",
  "main": "index.js",
  "scripts": {
    "doc": "jsdoc -r README.md src/ -d docs",
    "compile": "babel src/ -d lib/ -D -s & (browserify src/index.js -s mandelbrot -d -t [ babelify ] | uglifyjs > dist/mandelbrot-viewer.min.js)",
    "build": "npm run doc & npm run compile",
    "prestart": "npm prune && npm install && npm run build",
    "start": "http-server -o demo/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:theOutLiar/mandelbrot-viewer.git"
  },
  "keywords": [
    "mandelbrot",
    "fractal",
    "visualization"
  ],
  "author": "Christopher J Gore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christophergorexyz/mandelbrot-viewer/issues"
  },
  "homepage": "http://christophergorexyz.github.io/mandelbrot-viewer/",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-jsdoc": "^38.0.2",
    "http-server": "^13",
    "jsdoc": "^3.6.10",
    "uglify-js": "^3.15.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
